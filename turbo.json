{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "cache": false,
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "check:types": {
      "cache": false,
      "dependsOn": ["^check:types"]
    },
    "test:ci": {
      "cache": false,
      "dependsOn": ["build"]
    },
    "bench": {
      "cache": false,
      "dependsOn": ["build"]
    }
  }
}
